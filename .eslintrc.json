{
    "extends": "airbnb-base",
    "rules": {
        "import/prefer-default-export": "off",
        "import/extensions": [
            "error",
            "always",
            {
                "js": "never",
                "mjs": "never"
            }
        ]
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".mjs"]
            }
        }
    }
}
